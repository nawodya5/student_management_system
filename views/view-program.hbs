<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="/">Home</a></li>
        <li class="breadcrumb-item active" aria-current="page">View Program</li>
    </ol>
</nav>

<div class="view-user p-5">
    {{#each rows}}
    <div class="row mb-5">
        <div class="col text-center">
            <h3>{{this.program_name}}</h3>
        </div>
    </div>
    <div class="row">
        <div class="col">
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">program_id</th>
                        <th scope="col">program_name</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <th scope="row">{{this.program_id}}</th>
                        <td>{{this.program_name}}</td>
                    </tr>
                </tbody>
            </table>
        </div>
        <div class="row">
            <div class="col">
                <b>Description</b>
            </div>
        </div>
        <div class="row">
            <div class="col">
                {{this.description}}
            </div>
        </div>
    </div>
    {{/each}}
</div>